<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Jolin's Portofolio</title>
    <link rel="stylesheet" href="/styles/content.css" />
  </head>
  <body>
    <%- include("partials/header.ejs") %>

    <div class="container mt-5">
      <div class="row">
        <div class="col-lg-6 text-light">
          <h3 class="mb-3 text-center">Techinal Skill</h3>
          <div class="skill">
            <div class="skill-title">HTML, CSS, Bootstrap</div>
            <div class="skill-bar"></div>
            <div class="html skill-fill">
              <span class="skill-percent">95%</span>
            </div>
          </div>

          <div class="skill">
            <div class="skill-title">Javascript</div>
            <div class="skill-bar"></div>
            <div class="js skill-fill">
              <span class="skill-percent">90%</span>
            </div>
          </div>

          <div class="skill">
            <div class="skill-title">JQuery</div>
            <div class="skill-bar"></div>
            <div class="jquery skill-fill">
              <span class="skill-percent">90%</span>
            </div>
          </div>

          <div class="skill">
            <div class="skill-title">Python</div>
            <div class="skill-bar"></div>
            <div class="python skill-fill">
              <span class="skill-percent">90%</span>
            </div>
          </div>

          <div class="skill">
            <div class="skill-title">C</div>
            <div class="skill-bar"></div>
            <div class="c skill-fill">
              <span class="skill-percent">90%</span>
            </div>
          </div>

          <div class="skill">
            <div class="skill-title">Java</div>
            <div class="skill-bar"></div>
            <div class="java skill-fill">
              <span class="skill-percent">90%</span>
            </div>
          </div>
        </div>

        <div class="col-lg-6 ps-5">
          <h3 class="text-light text-center mb-3">Professional Skill</h3>
          <div class="outer-box">
            <div class="box text-lg-start text-center">
              <div class="percent">
                <svg>
                  <circle cx="70" cy="70" r="70"></circle>
                  <circle cx="70" cy="70" r="70"></circle>
                </svg>
                <div class="number">
                  <h2 class="text-percent">95<span>%</span></h2>
                </div>
              </div>
              <h5 class="text text-light">Attention in detail</h5>
            </div>
            <div class="box">
              <div class="percent">
                <svg>
                  <circle cx="70" cy="70" r="70"></circle>
                  <circle cx="70" cy="70" r="70"></circle>
                </svg>
                <div class="number">
                  <h2 class="text-percent">95<span>%</span></h2>
                </div>
              </div>
              <h5 class="text text-light">Time Management</h5>
            </div>
            <div class="box">
              <div class="percent">
                <svg>
                  <circle cx="70" cy="70" r="70"></circle>
                  <circle cx="70" cy="70" r="70"></circle>
                </svg>
                <div class="number">
                  <h2 class="text-percent">95<span>%</span></h2>
                </div>
              </div>
              <h5 class="text text-light">Creativity</h5>
            </div>

            <div class="box">
              <div class="percent">
                <svg>
                  <circle cx="70" cy="70" r="70"></circle>
                  <circle cx="70" cy="70" r="70"></circle>
                </svg>
                <div class="number">
                  <h2 class="text-percent">95<span>%</span></h2>
                </div>
              </div>
              <h5 class="text text-light">Problem Solving</h5>
            </div>
          </div>
        </div>
      </div>
    </div>

    <%- include("partials/footer.ejs") %>
    <script src="scripts/cursor.js"></script>
  </body>
</html>
